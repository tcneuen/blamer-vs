{
  "name": "blamer-vs",
  "displayName": "SVN Gutter (fork/tcneuen)",
  "description": "Visually blame SVN-stored code line-by-line",
  "version": "0.6.0",
  "publisher": "tcneuen",
  "repository": {
    "type": "git",
    "url": "https://github.com/tcneuen/blamer-vs.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "SCM Providers",
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.blameFile"
  ],
  "main": "./out/main.js",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.blameFile",
        "key": "ctrl+alt+d",
        "mac": "ctrl+cmd+x",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "extension.blameFile",
        "title": "SVN Gutter - Show Blame"
      },
      {
        "command": "extension.clearBlame",
        "title": "SVN Gutter - Clear Blame"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "SVN Gutter",
      "properties": {
        "svn-gutter.enablePips": {
          "type": "boolean",
          "default": true,
          "description": "Enable/Disable visual 'pip' decoration, that sits alongside the line number."
        },
        "svn-gutter.enableDetail": {
          "type": "boolean",
          "default": true,
          "description": "Enable/Disable SVN details from SVN log. Disabling this setting will significantly speed up blame."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./extension.js --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.11",
    "@types/vscode": "^1.60.0",
    "esbuild": "^0.12.28",
    "eslint": "^6.6.0",
    "mocha": "^6.2.2",
    "prettier": "^2.4.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "fast-xml-parser": "^3.20.0"
  }
}
